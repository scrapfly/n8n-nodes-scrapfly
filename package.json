{
  "name": "n8n-nodes-scrapfly",
  "version": "0.1.3",
  "description": "Scrapfly n8n community node for web page scraping, screenshots, and AI data extraction",
  "keywords": [
    "n8n-community-node-package",
    "scrapfly"
  ],
  "license": "MIT",
  "homepage": "https://scrapfly.io/",
  "author": {
    "name": "Mazen Ramadan",
    "email": "mazen@scrapfly.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scrapfly/n8n-nodes-scrapfly.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ScrapflyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Scrapfly/Scrapfly.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
